<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Configurations>Debug;Release;Signed</Configurations>
    <AssemblyName>UvDebug</AssemblyName>
    <RootNamespace>UvDebug</RootNamespace>
    <ApplicationIcon></ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Ultraviolet.BASS\NETStandard21\Ultraviolet.BASS.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.Core\NETStandard21\Ultraviolet.Core.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.FreeType2\NETStandard21\Ultraviolet.FreeType2.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.ImGuiViewProvider.Bindings\NETStandard21\Ultraviolet.ImGuiViewProvider.Bindings.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.ImGuiViewProvider\NETStandard21\Ultraviolet.ImGuiViewProvider.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.OpenGL.Bindings\NETStandard21\Ultraviolet.OpenGL.Bindings.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.OpenGL\NETStandard21\Ultraviolet.OpenGL.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.Presentation.Compiler\NETStandard21\Ultraviolet.Presentation.Compiler.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.Presentation.Uvss\NETStandard21\Ultraviolet.Presentation.Uvss.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.Presentation\NETStandard21\Ultraviolet.Presentation.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.SDL2\NETStandard21\Ultraviolet.SDL2.NETStandard21.csproj" />
    <ProjectReference Include="..\..\Ultraviolet.Shims.NETCore3\Ultraviolet.Shims.NETCore3.csproj" />
    <ProjectReference Include="..\..\Ultraviolet\NETStandard21\Ultraviolet.NETStandard21.csproj" />
  </ItemGroup>

  <PropertyGroup>
    <RunCreateSymbolicLinksToFreeType2Dependencies>false</RunCreateSymbolicLinksToFreeType2Dependencies>
    <RunCreateSymbolicLinksToFreeType2Dependencies Condition="'$(OS)' == 'Unix' And !Exists('/Library/Frameworks')">true</RunCreateSymbolicLinksToFreeType2Dependencies>
    <RunCreateSymbolicLinksToFreeType2Dependencies_osx>false</RunCreateSymbolicLinksToFreeType2Dependencies_osx>
    <RunCreateSymbolicLinksToFreeType2Dependencies_osx Condition="'$(OS)' == 'Unix' And Exists('/Library/Frameworks')">true</RunCreateSymbolicLinksToFreeType2Dependencies_osx>
    <StartupObject>UvDebug.Game</StartupObject>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;IMGUI</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;IMGUI</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Signed|AnyCPU'">
    <DefineConstants>TRACE;IMGUI</DefineConstants>
  </PropertyGroup>
  <Target Name="CreateSymbolicLinksToDependencies_x86" Condition=" '$(RunCreateSymbolicLinksToFreeType2Dependencies)' == 'true' " AfterTargets="AfterBuild">
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libfreetype.so.6.16.0&quot; &quot;$(TargetDir)/x86/unix/libfreetype.so.6&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libfreetype.so.6&quot; &quot;$(TargetDir)/x86/unix/libfreetype.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libharfbuzz.so.0.10706.0&quot; &quot;$(TargetDir)/x86/unix/libharfbuzz.so.0&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libharfbuzz.so.0&quot; &quot;$(TargetDir)/x86/unix/libharfbuzz.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libpng16.so.16.34.0&quot; &quot;$(TargetDir)/x86/unix/libpng16.so.16&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libpng16.so.16&quot; &quot;$(TargetDir)/x86/unix/libpng16.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x86/unix/libpng16.so&quot; &quot;$(TargetDir)/x86/unix/libpng.so&quot;" />
  </Target>
  <Target Name="CreateSymbolicLinksToDependencies_x64" Condition=" '$(RunCreateSymbolicLinksToFreeType2Dependencies)' == 'true' " AfterTargets="CreateSymbolicLinksToDependencies_x86">
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libfreetype.so.6.16.0&quot; &quot;$(TargetDir)/x64/unix/libfreetype.so.6&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libfreetype.so.6&quot; &quot;$(TargetDir)/x64/unix/libfreetype.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libharfbuzz.so.0.10706.0&quot; &quot;$(TargetDir)/x64/unix/libharfbuzz.so.0&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libharfbuzz.so.0&quot; &quot;$(TargetDir)/x64/unix/libharfbuzz.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libpng16.so.16.34.0&quot; &quot;$(TargetDir)/x64/unix/libpng16.so.16&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libpng16.so.16&quot; &quot;$(TargetDir)/x64/unix/libpng16.so&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)/x64/unix/libpng16.so&quot; &quot;$(TargetDir)/x64/unix/libpng.so&quot;" />
  </Target>
  <Target Name="CreateSymbolicLinksToDependencies_osx" Condition=" '$(RunCreateSymbolicLinksToFreeType2Dependencies_osx)' == 'true' " AfterTargets="AfterBuild">
    <Exec Command="ln -sf &quot;$(TargetDir)x64/osx/libfreetype.6.dylib&quot; &quot;$(TargetDir)x64/osx/libfreetype.dylib&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)x64/osx/libharfbuzz.0.dylib&quot; &quot;$(TargetDir)x64/osx/libharfbuzz.dylib&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)x64/osx/libpng16.16.dylib&quot; &quot;$(TargetDir)x64/osx/libpng16.dylib&quot;" />
    <Exec Command="ln -sf &quot;$(TargetDir)x64/osx/libpng16.dylib&quot; &quot;$(TargetDir)x64/osx/libpng.dylib&quot;" />
  </Target>

  <Import Project="..\Shared\UvDebug.projitems" Label="Shared" />

  
</Project>
