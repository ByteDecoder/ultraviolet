<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\armeabi-v7a\libfreetype.so">
      <Link>lib\armeabi-v7a\libfreetype.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\armeabi-v7a\libharfbuzz.so">
      <Link>lib\armeabi-v7a\libharfbuzz.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\armeabi-v7a\libpng16.so">
      <Link>lib\armeabi-v7a\libpng16.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\x86\libfreetype.so">
      <Link>lib\x86\libfreetype.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\x86\libharfbuzz.so">
      <Link>lib\x86\libharfbuzz.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)lib\x86\libpng16.so">
      <Link>lib\x86\libpng16.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </AndroidNativeLibrary>  
  </ItemGroup>
  <PropertyGroup>  
    <AndroidSupportedAbis>armeabi-v7a,x86</AndroidSupportedAbis>
  </PropertyGroup>
</Project>